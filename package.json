{
  "name": "snaptrackmobile",
  "version": "1.2.1",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:android:local": "cd android && ./gradlew assembleRelease",
    "pre-release": "./scripts/pre-release-checks.sh",
    "pre-release:quick": "./scripts/pre-release-checks.sh --quick",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "security-check": "npm audit --audit-level=high",
    "build:test": "expo export --platform ios --output-dir ./build-test --experimental-bundle",
    "release:prepare": "npm run pre-release && echo 'Ready for EAS build!'",
    "release:build": "npm run pre-release && eas build --platform all --auto-submit",
    "release:ios": "./scripts/release-ios.sh",
    "release:android": "./scripts/release-android.sh",
    "release": "./scripts/release.sh",
    "qa-test": "./scripts/qa-test.sh",
    "qa-android": "./scripts/qa-android.sh",
    "qa-ios": "./scripts/qa-ios.sh",
    "qa-build": "npm run pre-release && eas build --profile development --platform all"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "expo": "53.0.19",
    "expo-apple-authentication": "~7.2.4",
    "expo-camera": "~16.1.10",
    "expo-clipboard": "^7.1.5",
    "expo-constants": "^17.1.7",
    "expo-crypto": "^14.1.5",
    "expo-device": "^7.1.4",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-haptics": "~14.1.4",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-status-bar": "~2.2.3",
    "firebase": "^11.10.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^18.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.9",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
